{
  "name": "chrome扩展测试demo",
  "version": "1.0",
  "manifest_version": 2,
  "description": "chrome扩展测试。",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
      "downloads",
      "activeTab"
  ]
}